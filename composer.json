{
    "name": "spykeer/glpi-assetscleaner",
    "description": "GLPI plugin to automatically clean old assets not updated by inventory",
    "type": "glpi-plugin",
    "keywords": ["glpi", "plugin", "assets", "cleaner", "inventory"],
    "homepage": "https://github.com/SpyKeeR/Glpi-AssetsCleaner",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "SpyKeeR"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "glpi-project/tools": "^0.8"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.99"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "GlpiPlugin\\Assetscleaner\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Glpi\\Tools\\": "../../tools/src/"
        }
    }
}
